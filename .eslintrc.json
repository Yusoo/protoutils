{
  "env": {
    "browser": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "parser": "babel-eslint",
    "sourceType": "module",
    "ecmaVersion": 8
  },
  "rules": {
    "no-console": 1,
    "no-undef": 0,
    "eqeqeq": 1,
    "no-unreachable": 2,
    "indent-legacy": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "quotes": [
      1,
      "single"
    ],
    "semi": [
      2,
      "never"
    ],
    "comma-dangle": [
      1,
      "always-multiline"
    ]
  }
}
